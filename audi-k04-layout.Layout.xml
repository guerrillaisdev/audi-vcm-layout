  <!-- Math Channels -->
  <Content ID="6" Name="Math" Parent="2" DockAlignment="4" Proportion="0.5" IsActiveContent="True" Text="Math Channels">
    <MathChannel>
      <Name>Calc_AFR</Name>
      <Formula>P50120 * 14.7</Formula>
      <Description>Calculated AFR from Lambda</Description>
      <Unit>150</Unit>
      <Decimals>1</Decimals>
    </MathChannel>
    <MathChannel>
      <Name>Calc_BoostPSI</Name>
      <Formula>(P50030 - 101.325) * 0.145</Formula>
      <Description>Boost PSI conversion</Description>
      <Unit>91</Unit>
      <Decimals>1</Decimals>
    </MathChannel>
    <MathChannel>
      <Name>Calc_InjDuty</Name>
      <Formula>(P50112 * P50070 * 0.12) / 60000</Formula>
      <Description>Injector duty cycle</Description>
      <Unit>156</Unit>
      <Decimals>1</Decimals>
    </MathChannel>
  </Content>
  
  <!-- Triggers -->
  <Content ID="7" Name="Triggers" Parent="2" DockAlignment="4" Proportion="0.5" IsActiveContent="True" Text="Triggers">
    <Trigger>
      <Name>WOT_Logging</Name>
      <Condition>P50090 > 80</Condition>
      <PreTrigger>5</PreTrigger>
      <PostTrigger>10</PostTrigger>
      <FilePrefix>WOT_</FilePrefix>
      <Description>Wide Open Throttle logging</Description>
    </Trigger>
    <Trigger>
      <Name>Knock_Event</Name>
      <Condition>P50111 > 2</Condition>
      <PreTrigger>3</PreTrigger>
      <PostTrigger>7</PostTrigger>
      <FilePrefix>Knock_</FilePrefix>
      <Description>Knock event detection</Description>
    </Trigger>
    <Trigger>
      <Name>High_Boost</Name>
      <Condition>P50030 > 20</Condition>
      <PreTrigger>3</PreTrigger>
      <PostTrigger>7</PostTrigger>
      <FilePrefix>Boost_</FilePrefix>
